#include<stdio.h>
int main()
{
    int a,b,c=0,d=0;
    scanf("%d %d",&a,&b);
    c=a;
    c=c*3;
    d=b;
    d=d*2;
    for(int i=1;i<=10;i++)
    {
        if(c>d)
        {
            printf("%d",i);
            break;
        }
        else
        {
            c=c*3;
            d=d*2;
        }
    }
}
